{
  "name": "@smui/common",
  "version": "7.0.0-beta.0",
  "description": "Svelte Material UI - Common",
  "type": "module",
  "svelte": "./dist/index.js",
  "exports": {
    "./package.json": "./package.json",
    ".": "./dist/index.js",
    "./classadder": "./dist/classadder/index.js",
    "./internal": "./dist/internal/index.js"
  },
  "typesVersions": {
    "*": {
      "classadder": [
        "./src/classadder/index.ts"
      ],
      "internal": [
        "./src/internal/index.ts"
      ],
      "*": [
        "./src/*"
      ]
    }
  },
  "keywords": [
    "svelte",
    "svelte3",
    "material-ui",
    "material-design",
    "material",
    "svelte-components",
    "sveltejs"
  ],
  "scripts": {
    "build": "npm run build:js && npm run build:svelte",
    "build:js": "tsc -p . --outDir dist/ --rootDir src/",
    "build:svelte": "svelte-strip strip src/ dist",
    "clean": "git status --ignored -u --porcelain . | grep -v node_modules/ | grep -e '^!! ' | sed 's/^!! packages\\/[a-z-]*\\///g' | tr '\\n' ' ' | xargs rm",
    "check": "npx svelte-check --ignore dist",
    "prepare": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hperrin/svelte-material-ui.git"
  },
  "author": "Hunter Perrin <hperrin@gmail.com>",
  "bugs": {
    "url": "https://github.com/hperrin/svelte-material-ui/issues"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "sass": "^1.54.0",
    "svelte-check": "^2.8.1",
    "svelte-strip": "^1.0.2",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@material/dom": "^14.0.0",
    "@tsconfig/svelte": "^3.0.0",
    "svelte2tsx": "^0.5.12"
  },
  "gitHead": "c61a67b26e61d6efdc76b8b6db2087793eecb9fd"
}
